### PostgreSQL database configuration
GCP_CLOUDSQL_HOST=GCP-Cloud-Host-IP-Address
GCP_CLOUDSQL_PORT=5432
GCP_CLOUDSQL_SSL_MODE=require
POSTGRES_USER=postgres
POSTGRES_PASSWORD="databasePassword-should_be_in_quotes"
POSTGRES_DB=postgres

### Redis Cloud configuration
REDIS_CLOUD_HOST=your-redis-cloud-host-endpoint
REDIS_CLOUD_PORT=11048
REDIS_CLOUD_USERNAME=default
REDIS_CLOUD_PASSWORD=redits-cloud-password

### n8n Authentication & Security
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=IamSherlocked
N8N_ENCRYPTION_KEY=create-random-encryption-key
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true
EXECUTIONS_MODE=queue
N8N_RUNNERS_ENABLED=true
N8N_RUNNERS_MODE=internal
N8N_RUNNERS_AUTH_TOKEN=create-auth-token-key
N8N_WORKERS_ENABLED=true
OFFLOAD_MANUAL_EXECUTIONS_TO_WORKERS=true
N8N_WORKER_CONCURRENCY=10

### Worker/Queue/Database configuration
N8N_WORKER_TYPE=main
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=${GCP_CLOUDSQL_HOST}
DB_POSTGRESDB_PORT=${GCP_CLOUDSQL_PORT}
DB_POSTGRESDB_DATABASE=${POSTGRES_DB}
DB_POSTGRESDB_USER=${POSTGRES_USER}
DB_POSTGRESDB_PASSWORD=${POSTGRES_PASSWORD}

### Redis Configuration - Optimized for connection pooling
QUEUE_BULL_REDIS_HOST=${REDIS_CLOUD_HOST}
QUEUE_BULL_REDIS_PORT=${REDIS_CLOUD_PORT}
QUEUE_BULL_REDIS_USERNAME=${REDIS_CLOUD_USERNAME}
QUEUE_BULL_REDIS_PASSWORD=${REDIS_CLOUD_PASSWORD}
QUEUE_HEALTH_CHECK_ACTIVE=true

### Redis Connection Pool Settings
QUEUE_BULL_REDIS_DB=0
QUEUE_BULL_REDIS_TIMEOUT_THRESHOLD=10000
QUEUE_BULL_REDIS_MAX_STALENESS=30
QUEUE_BULL_REDIS_MAX_CONNECTIONS=5
QUEUE_BULL_REDIS_CLUSTER_SETTINGS={"enableReadyCheck":false,"maxRetriesPerRequest":3,"lazyConnect":true,"keepAlive":30000,"family":4}

### SMTP settings for email
N8N_EMAIL_MODE=smtp
N8N_SMTP_HOST=smtp.gmail.com
N8N_SMTP_PORT=587
N8N_SMTP_USER=youremail@gmail.com
N8N_SMTP_PASS=email-app-password
N8N_SMTP_SENDER="Sender Name <youremail@gmail.com>"
N8N_SMTP_SSL=false
N8N_SMTP_STARTTLS=true

### n8n Core Configuration - Local Development
N8N_HOST=localhost
N8N_PROTOCOL=http
N8N_EDITOR_BASE_URL=http://localhost:5678
WEBHOOK_URL=http://localhost:5678/
TIMEZONE=Asia/Kolkata
N8N_SECURE_COOKIE=false
N8N_SKIP_WEBHOOK_DEREGISTRATION_SHUTDOWN=true